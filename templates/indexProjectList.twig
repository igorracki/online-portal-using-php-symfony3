<h3 class="info"> List of All Projects </h3>
<table align="center">
    <tr>
        <th>ID</th>
        <th>Name</th>
        <th>Locked</th>
        <th> </th>
    </tr>
    {% for project in allProjects %}
        <tr>
            <td> {{ project.id }}</td>
            <td> {{ project.name }}</td>
            <td class="infoStatus">
                {% if project.locked == 0 %}
                Unlocked
                {% elseif project.locked == 1 %}
                Locked
                {% endif %}
            </td>
            <td><a class="button" href="/project/{{ project.id }}">View Project</a></td>
        </tr>
    {% endfor %}
</table>